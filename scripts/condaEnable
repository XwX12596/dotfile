__conda_setup="$('/home/xwx/.anaconda/bin/conda' 'shell.zsh' 'hook' 2> /dev/null)"
if [ $? -eq 0 ]; then
    eval "$__conda_setup"
else
    if [ -f "/home/xwx/.anaconda/etc/profile.d/conda.sh" ]; then
        . "/home/xwx/.anaconda/etc/profile.d/conda.sh"
    else
        export PATH="/home/xwx/.anaconda/bin:$PATH"
    fi
fi
unset __conda_setup
